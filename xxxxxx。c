#include <stdio.h>
#include <math.h>
int main(){
	int n;
	scanf("%d",&n);
	for(int i=0;i<n;i++)
	{int k;
	scanf("%d",&k);
	int c=k;
	  for(int j=2;j<=k;j++)
	  { if(c%j==0)  
	    for(int p=0;;p++)
		{
			c=c/j;
			printf("%d",j);
			if(c!=1)
			printf("*");
			if(c%j!=0)
			break;
		}
	    
	  
	  	
	  }	printf("\n");
	}
} 
