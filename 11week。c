#include<stdio.h>
#include <math.h>
#include <stdlib.h>
int main(){
	int n,m;
	scanf("%d%d",&n,&m);//注意这里 
	int x;
	scanf("%d",&x);
	int a[1000];
	for(int i=0;i<n;i++)
	{ scanf("%d",&a[i]);} //这别再写inta【i】了 
	 if(x==1)
	 for(int i=0;i<m;i++)
	 
	 {int y=a[0];
	     //两个等于号 
	  {
	  for(int j=1;j<n;j++)
	    {a[j-1]=a[j];
	   
	    }
	    a[n-1]=y;
		}}
		   	 if(x==0){
				
	 for(int i=0;i<m;i++)//两个等于号 
	{
	      int y=a[n-1];
	  for(int j=n-2;j>=0;j--)
	    {a[j+1]=a[j];
	   
	    }
	    a[0]=y;
		}
		}
		
		
		
		printf("%d ",a[0]);
		for(int i=1;i<n;i++)
		printf("%d ",a[i]);
		

	 
}
