#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <string.h>
int main(){
	int n;
	scanf("%d",&n);
if(n<=0||n>=8)
printf("Input Error");


if(n>0&&n<8){

	if(n==1)
	printf("2");
	if(n==2);
	printf("4");
	if(n==3);
	printf("5");
	if(n==4);
	printf("4");
	if(n==5);
	printf("2");
	if(n==6);
	printf("3");
	if(n==7);
	printf("0");}
