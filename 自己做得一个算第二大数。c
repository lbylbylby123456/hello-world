#include <stdio.h>
#include <math.h>
#include <stdlib.h>
int main(){
	int n;
	scanf("%d",&n);
	int x[10000];
	int a=x[0]; 
	int b;
	for (int i=0;i<n;i++){
		scanf("%d",&x[i]);
	
        if(x[i]>x[i-1]){//不要写i+1>i 
        	a=x[i];
		}
		else a=a;}
		 
    for (int i=0;i<n;i++) {
    	if(x[i]!=a)
    	{if(x[i]>x[i-1]){//不要写i+1<i 
         b=x[i];
		}
		else b=a;}
    		
		}
	
	if (n<2) 
	printf("error");
  else	
  printf("%d",b);
	
}
