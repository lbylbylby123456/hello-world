#include <stdio.h>
#include <math.h>
#include <stdlib.h>
#include <string.h>
int main(){
	int n,k,b=1,tem;
	int i;
	scanf("%d",&n);
	int a[10000];//
	for( i=0;i<n;i++){
		scanf("%d",&a[i]);
		int x=a[i];
		for(int j=1+i;j<n;j++){
			if(a[j]<x)
			{
				x=a[j];
				k=j;
				b=0;
			}
			if(b==0){
			a[k]=tem;
			a[k]=a[i];
			a[i]=tem;	
			}
			
		}
		for(int p=0;p<n;p++){
			printf("%d",a[p]);}
		}
	}
	
