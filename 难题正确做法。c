#include <stdio.h>
#include <math.h>
int main (){
	int i;int t;int tem=0;int n;int m;
	int a;int b;int c;int d;int e;
	  int x;int y;int z;int w;
		
  for (i=0;i<=999999;i=i+1){
  	
  		//printf("%d\n",i);
  	
		//a>=1&&a<10;
		//b>=1&&b<10;
		//c>=1&&c<10;
	  //d>=1&&d<10;
	//	e>=1&&e<10;
	m=i/100000;
	n=i-100000*m;
	if(m==1)
	tem=tem+1;
	if(m!=1)
	tem=tem;
		a=n/10000;
		x=n-10000*a;
			//printf("%d",a);
		if(a==1) 
		{
		tem=tem+1;}
	
		if (a!=1)
		{
		tem=tem;} 
	//	printf("%d\n",tem);
		
		b=x/1000;
		y=x-1000*b;
		if (b==1) {
		tem=tem+1;}
		if (b!=1) {
		tem=tem;}
	//	printf("%d\n",tem);
		c=y/100;
		z=y-100*c;
		if (c==1) {
		tem=tem+1;}
	if(c!=1) {
			tem=tem;
		}
		// printf("%d\n",tem); 
		d=z/10;
		w=z-10*d;
		if (d==1) {
		tem=tem+1;}
		
	if(d!=1) {
	tem=tem;}
	//printf("%d\n",tem);
		e=w;
		if (e==1) {
		tem=tem+1;}
		if(e!=1) {
		tem=tem;
	}t=tem;
	//printf("%d\n",tem);
	
		
	while (i==t&&i!=0){
			 printf ("%d\n",i);
			 break;}		 }
		
		return 0;
	} 
	
